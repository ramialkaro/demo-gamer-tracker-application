{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "wwwroot/app",
    "lib": [ "es2015", "dom" ],
    "jsx":"react",
    "typeRoots": [
      "../node_modules/@types/"
    ]	
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "server",
    "server_node_modules"
  ]
}